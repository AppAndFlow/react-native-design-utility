{
  "name": "react-native-design-utility",
  "version": "1.0.0",
  "description": "react-native utility library for ui",
  "main": "dist/commonjs/index.js",
  "module": "dist/module/index.js",
  "react-native": "dist/module/index.js",
  "types": "dist/typescript/index.d.ts",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/EQuimper/react-native-design-utility"
  },
  "bugs": {
    "url": "https://github.com/EQuimper/react-native-design-utility/issues"
  },
  "homepage": "https://github.com/EQuimper/react-native-design-utility#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "@react-native-community/bob": {
    "source": "src",
    "output": "dist",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "keywords": [
    "react-native",
    "design-system",
    "design-utility",
    "theme"
  ],
  "files": [
    "dist/",
    "LICENSE.md",
    "README.md"
  ],
  "author": {
    "email": "quimperemanuel@gmail.com",
    "name": "Emanuel Quimper <EQuimper>"
  },
  "scripts": {
    "build": "bob build",
    "prepare": "yarn build",
    "release": "release-it",
    "postinstall": "npx typesync",
    "prettier": "prettier --write 'src/**/*'",
    "typescript": "tsc --noEmit"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "@react-native-community/bob": "^0.8.0",
    "@release-it/conventional-changelog": "^1.1.0",
    "@types/fs-extra": "^8.0.1",
    "@types/lodash.get": "^4.4.6",
    "@types/prettier": "^1.19.0",
    "@types/react": "^16.9.17",
    "@types/react-native": "^0.60.27",
    "@types/react-test-renderer": "^16.9.1",
    "@types/rimraf": "^2.0.3",
    "cross-env": "^6.0.3",
    "husky": "^4.0.9",
    "prettier": "^1.19.1",
    "react": "16.8.3",
    "react-native": "~0.59.8",
    "react-test-renderer": "^16.4.2",
    "release-it": "^12.4.3",
    "rimraf": "^3.0.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.1.0",
    "typescript": "^3.7.4"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native": ">=0.59.0"
  },
  "dependencies": {
    "fs-extra": "^8.1.0",
    "lodash.get": "^4.4.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
